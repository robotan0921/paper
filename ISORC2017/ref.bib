@article {par:Contiki,
	author = {Dutta, Prabal and Dunkels, Adam},
	title = {Operating systems and network protocols for wireless sensor networks},
	volume = {370},
	number = {1958},
	pages = {68--84},
	year = {2011},
	doi = {10.1098/rsta.2011.0330},
	publisher = {The Royal Society},
	abstract = {Sensor network protocols exist to satisfy the communication needs of diverse applications, including data collection, event detection, target tracking and control. Network protocols to enable these services are constrained by the extreme resource scarcity of sensor nodes{\textemdash}including energy, computing, communications and storage{\textemdash}which must be carefully managed and multiplexed by the operating system. These challenges have led to new protocols and operating systems that are efficient in their energy consumption, careful in their computational needs and miserly in their memory footprints, all while discovering neighbours, forming networks, delivering data and correcting failures.},
	issn = {1364-503X},
	URL = {http://rsta.royalsocietypublishing.org/content/370/1958/68},
	eprint = {http://rsta.royalsocietypublishing.org/content/370/1958/68.full.pdf},
	journal = {Philosophical Transactions of the Royal Society of London A: Mathematical, Physical and Engineering Sciences}
}
